print("UI library by xHeptc")
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Animation gui", "Synapse")
local Idle = Window:NewTab("Idle animations")

